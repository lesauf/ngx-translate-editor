{
  "name": "ngx-translate-editor",
  "version": "0.1.0",
  "scripts":
  {
    "ng": "ng",
    "startng": "ng serve --host 0.0.0.0 --port 8080 --disableHostCheck",
    "build": "ng build && cd server && tsc",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "startserver": "node_modules/.bin/ts-node-dev -r esm server/index.ts",
    "start": "concurrently -k -c \"yellow.bold,green.bold\" -n \"BUILD,SERVER\" \"ng build --aot --watch\" \"node_modules/.bin/ts-node-dev -r esm server/index.ts\"",
    "startold": "concurrently -k -c \"yellow.bold,green.bold\" -n \"ANGULAR,EXPRESS\" \"ng serve --host 0.0.0.0 --port 8080 --disableHostCheck\" \"node_modules/.bin/ts-node-dev -r esm server/index.ts\""
  },
  "keywords": [
    "Angular",
    "ngx-translate-editor",
    "Translation"
  ],
  "author": "Fotso Teketchuet Stephane",
  "license": "MIT",
  "private": true,
  "dependencies":
  {
    "@angular/animations": "~9.0.0",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~9.0.0",
    "@angular/material": "^9.0.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "@nativescript/schematics": "^1.0.0",
    "compression": "^1.7.4",
    "concurrently": "^5.1.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "flat": "^5.0.0",
    "hammerjs": "^2.0.8",
    "lodash": "^4.17.15",
    "ng-mocks": "^9.0.0",
    "rxjs": "^6.5.4",
    "ts-node": "^8.6.2",
    "tslib": "^1.10.0",
    "typescript": "^3.7.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies":
  {
    "@angular-builders/jest": "^8.3.2",
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "^9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@ngneat/spectator": "^5.3.1",
    "@types/express": "^4.17.2",
    "@types/flat": "0.0.28",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.0",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "codelyzer": "^5.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.6.0",
    "jest": "^25.1.0",
    "protractor": "~5.4.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-node-dev": "^1.0.0-pre.44"
  }
}
